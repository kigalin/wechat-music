page {
  background-color: #66bab7;
  height: 100%;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #66bab7;
}

.avatar {
  width: 400rpx;
  height: 400rpx;
  margin-top: 100rpx;
}

text {
  font-size: 45rpx;
  line-height: 80rpx;
  text-align: right;
  color: #fedfe1;
  font-weight: bold;
  letter-spacing: 2rpx;
}

.start-container {
  margin-top: 160rpx;
  border: 1px solid #0089a7;
  border-radius: 15rpx;
  width: 200rpx;
  height: 80rpx;
  text-align: center;
  background-color: #3a8fb7;
}

.start{
  letter-spacing: 6rpx;
}